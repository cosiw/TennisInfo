CREATE TABLE `tb_career` (
	`CR_ID` BIGINT(20) NOT NULL AUTO_INCREMENT,
	`PLAYER_ID` BIGINT(20) NULL DEFAULT NULL,
	`CR_MATCH_LOST_D` VARCHAR(50) NULL DEFAULT NULL COLLATE 'utf8mb4_general_ci',
	`CR_MATCH_LOST_S` VARCHAR(50) NULL DEFAULT NULL COLLATE 'utf8mb4_general_ci',
	`CR_MATCH_WIN_D` VARCHAR(50) NULL DEFAULT NULL COLLATE 'utf8mb4_general_ci',
	`CR_MATCH_WIN_S` VARCHAR(50) NULL DEFAULT NULL COLLATE 'utf8mb4_general_ci',
	`CR_HIGH_D` VARCHAR(50) NULL DEFAULT NULL COLLATE 'utf8mb4_general_ci',
	`CR_HIGH_S` VARCHAR(50) NULL DEFAULT NULL COLLATE 'utf8mb4_general_ci',
	`CR_HIGH_DT_D` VARCHAR(50) NULL DEFAULT NULL COLLATE 'utf8mb4_general_ci',
	`CR_HIGH_DT_S` VARCHAR(50) NULL DEFAULT NULL COLLATE 'utf8mb4_general_ci',
	`CR_PRZ` VARCHAR(50) NULL DEFAULT NULL COLLATE 'utf8mb4_general_ci',
	`PRZ_YTD_D` VARCHAR(50) NULL DEFAULT NULL COLLATE 'utf8mb4_general_ci',
	`PRZ_YTD_S` VARCHAR(50) NULL DEFAULT NULL COLLATE 'utf8mb4_general_ci',
	`RANK_D` VARCHAR(50) NULL DEFAULT NULL COLLATE 'utf8mb4_general_ci',
	`RANK_S` VARCHAR(50) NULL DEFAULT NULL COLLATE 'utf8mb4_general_ci',
	`CR_TITLE_D` VARCHAR(50) NULL DEFAULT NULL COLLATE 'utf8mb4_general_ci',
	`CR_TITLE_S` VARCHAR(50) NULL DEFAULT NULL COLLATE 'utf8mb4_general_ci',
	`TITLE_YTD_D` VARCHAR(50) NULL DEFAULT NULL COLLATE 'utf8mb4_general_ci',
	`TITLE_YTD_S` VARCHAR(50) NULL DEFAULT NULL COLLATE 'utf8mb4_general_ci',
	`MATCH_YTD_L_D` VARCHAR(50) NULL DEFAULT NULL COLLATE 'utf8mb4_general_ci',
	`MATCH_YTD_L_S` VARCHAR(50) NULL DEFAULT NULL COLLATE 'utf8mb4_general_ci',
	`MATCH_YTD_W_D` VARCHAR(50) NULL DEFAULT NULL COLLATE 'utf8mb4_general_ci',
	`MATCH_YTD_W_S` VARCHAR(50) NULL DEFAULT NULL COLLATE 'utf8mb4_general_ci',
	`CRT_DT` DATETIME NULL DEFAULT current_timestamp(),
	`UPD_DT` DATETIME NULL DEFAULT current_timestamp(),
	PRIMARY KEY (`CR_ID`) USING BTREE
)
COLLATE='utf8mb4_general_ci'
ENGINE=InnoDB
AUTO_INCREMENT=997
;


CREATE TABLE `tb_category` (
	`CATEGORY_ID` BIGINT(20) NOT NULL AUTO_INCREMENT,
	`RAPID_CATEGORY_ID` VARCHAR(10) NULL DEFAULT NULL COLLATE 'utf8mb4_general_ci',
	`CATEGORY_NAME` VARCHAR(300) NULL DEFAULT NULL COLLATE 'utf8mb4_general_ci',
	`CRT_DT` DATETIME NULL DEFAULT current_timestamp(),
	`UPD_DT` DATETIME NULL DEFAULT current_timestamp(),
	PRIMARY KEY (`CATEGORY_ID`) USING BTREE
)
COLLATE='utf8mb4_general_ci'
ENGINE=InnoDB
AUTO_INCREMENT=37
;

CREATE TABLE `tb_match` (
	`MATCH_ID` BIGINT(20) NOT NULL AUTO_INCREMENT,
	`ROUND_ID` BIGINT(20) NOT NULL DEFAULT '0',
	`RAPID_MATCH_ID` VARCHAR(50) NOT NULL DEFAULT '0' COLLATE 'utf8mb4_general_ci',
	`HOME_SEED` VARCHAR(50) NULL DEFAULT '0' COLLATE 'utf8mb4_general_ci',
	`AWAY_SEED` VARCHAR(50) NULL DEFAULT '0' COLLATE 'utf8mb4_general_ci',
	`HOME_SCORE` TINYINT(4) NULL DEFAULT '0',
	`AWAY_SCORE` TINYINT(4) NULL DEFAULT '0',
	`HOME_PLAYER` BIGINT(20) NULL DEFAULT '0',
	`AWAY_PLAYER` BIGINT(20) NULL DEFAULT '0',
	`HOME_SET1` TINYINT(4) NULL DEFAULT NULL,
	`HOME_SET2` TINYINT(4) NULL DEFAULT NULL,
	`HOME_SET3` TINYINT(4) NULL DEFAULT NULL,
	`HOME_SET4` TINYINT(4) NULL DEFAULT NULL,
	`HOME_SET5` TINYINT(4) NULL DEFAULT NULL,
	`AWAY_SET1` TINYINT(4) NULL DEFAULT NULL,
	`AWAY_SET2` TINYINT(4) NULL DEFAULT NULL,
	`AWAY_SET3` TINYINT(4) NULL DEFAULT NULL,
	`AWAY_SET4` TINYINT(4) NULL DEFAULT NULL,
	`AWAY_SET5` TINYINT(4) NULL DEFAULT NULL,
	`PERIOD_SET1` VARCHAR(10) NULL DEFAULT NULL COLLATE 'utf8mb4_general_ci',
	`PERIOD_SET2` VARCHAR(10) NULL DEFAULT NULL COLLATE 'utf8mb4_general_ci',
	`PERIOD_SET3` VARCHAR(10) NULL DEFAULT NULL COLLATE 'utf8mb4_general_ci',
	`PERIOD_SET4` VARCHAR(10) NULL DEFAULT NULL COLLATE 'utf8mb4_general_ci',
	`PERIOD_SET5` VARCHAR(10) NULL DEFAULT NULL COLLATE 'utf8mb4_general_ci',
	`HOME_TIE_SET1` TINYINT(4) NULL DEFAULT NULL,
	`HOME_TIE_SET2` TINYINT(4) NULL DEFAULT NULL,
	`HOME_TIE_SET3` TINYINT(4) NULL DEFAULT NULL,
	`HOME_TIE_SET4` TINYINT(4) NULL DEFAULT NULL,
	`HOME_TIE_SET5` TINYINT(4) NULL DEFAULT NULL,
	`AWAY_TIE_SET1` TINYINT(4) NULL DEFAULT NULL,
	`AWAY_TIE_SET2` TINYINT(4) NULL DEFAULT NULL,
	`AWAY_TIE_SET3` TINYINT(4) NULL DEFAULT NULL,
	`AWAY_TIE_SET4` TINYINT(4) NULL DEFAULT NULL,
	`AWAY_TIE_SET5` TINYINT(4) NULL DEFAULT NULL,
	`START_TIMESTAMP` VARCHAR(50) NULL DEFAULT NULL COLLATE 'utf8mb4_general_ci',
	`WINNER` VARCHAR(2) NULL DEFAULT NULL COLLATE 'utf8mb4_general_ci',
	`CRT_DT` DATETIME NULL DEFAULT current_timestamp(),
	`UPD_DT` DATETIME NULL DEFAULT current_timestamp(),
	PRIMARY KEY (`MATCH_ID`) USING BTREE
)
COLLATE='utf8mb4_general_ci'
ENGINE=InnoDB
AUTO_INCREMENT=25285
;

CREATE TABLE `tb_player` (
	`PLAYER_ID` BIGINT(20) NOT NULL AUTO_INCREMENT,
	`RAPID_PLAYER_ID` VARCHAR(10) NULL DEFAULT NULL COLLATE 'utf8mb4_general_ci',
	`PLAYER_NAME` VARCHAR(50) NULL DEFAULT NULL COLLATE 'utf8mb4_general_ci',
	`BIRTH` VARCHAR(10) NULL DEFAULT NULL COLLATE 'utf8mb4_general_ci',
	`COUNTRY` VARCHAR(500) NULL DEFAULT NULL COLLATE 'utf8mb4_general_ci',
	`TURNED_PRO` VARCHAR(4) NULL DEFAULT NULL COLLATE 'utf8mb4_general_ci',
	`WEIGHT` VARCHAR(10) NULL DEFAULT NULL COLLATE 'utf8mb4_general_ci',
	`PLAYS` VARCHAR(20) NULL DEFAULT NULL COLLATE 'utf8mb4_general_ci',
	`HEIGHT` VARCHAR(10) NULL DEFAULT NULL COLLATE 'utf8mb4_general_ci',
	`PRIZE_CURRENT` BIGINT(20) NULL DEFAULT NULL,
	`PRIZE_TOTAL` BIGINT(20) NULL DEFAULT NULL,
	`IMAGE` TEXT NULL DEFAULT NULL COLLATE 'utf8mb4_general_ci',
	`CRT_DT` DATETIME NULL DEFAULT current_timestamp(),
	`UPD_DT` DATETIME NULL DEFAULT current_timestamp(),
	PRIMARY KEY (`PLAYER_ID`) USING BTREE
)
COLLATE='utf8mb4_general_ci'
ENGINE=InnoDB
AUTO_INCREMENT=4945
;

CREATE TABLE `tb_ranking` (
	`RANKING_ID` BIGINT(20) NOT NULL AUTO_INCREMENT,
	`PLAYER_ID` BIGINT(20) NULL DEFAULT NULL,
	`CUR_RANKING` SMALLINT(6) NULL DEFAULT NULL,
	`PRE_RANKING` SMALLINT(6) NULL DEFAULT NULL,
	`BEST_RANKING` SMALLINT(6) NULL DEFAULT NULL,
	`CUR_POINTS` INT(11) NULL DEFAULT NULL,
	`PRE_POINTS` INT(11) NULL DEFAULT NULL,
	`RANKING_LAST_UPDATED` VARCHAR(50) NULL DEFAULT NULL COLLATE 'utf8mb4_general_ci',
	`CRT_DT` DATETIME NULL DEFAULT current_timestamp(),
	`UPD_DT` DATETIME NULL DEFAULT current_timestamp(),
	PRIMARY KEY (`RANKING_ID`) USING BTREE
)
COLLATE='utf8mb4_general_ci'
ENGINE=InnoDB
AUTO_INCREMENT=2594
;

CREATE TABLE `tb_round` (
	`ROUND_ID` BIGINT(20) NOT NULL AUTO_INCREMENT,
	`SEASON_ID` BIGINT(20) NOT NULL DEFAULT '0',
	`ROUND_RAPID_ID` VARCHAR(50) NOT NULL COLLATE 'utf8mb4_general_ci',
	`NAME` VARCHAR(50) NULL DEFAULT NULL COLLATE 'utf8mb4_general_ci',
	`SLUG` VARCHAR(50) NULL DEFAULT NULL COLLATE 'utf8mb4_general_ci',
	`CRT_DT` DATETIME NULL DEFAULT current_timestamp(),
	`UPD_DT` DATETIME NULL DEFAULT current_timestamp(),
	PRIMARY KEY (`ROUND_ID`) USING BTREE
)
COLLATE='utf8mb4_general_ci'
ENGINE=InnoDB
AUTO_INCREMENT=6114
;


CREATE TABLE `tb_season` (
	`SEASON_ID` BIGINT(20) NOT NULL AUTO_INCREMENT,
	`TOURNAMENT_ID` BIGINT(20) NULL DEFAULT NULL,
	`RAPID_SEASON_ID` VARCHAR(50) NULL DEFAULT NULL COLLATE 'utf8mb4_general_ci',
	`YEAR` VARCHAR(4) NULL DEFAULT NULL COLLATE 'utf8mb4_general_ci',
	`TOTAL_PRIZE` BIGINT(20) NULL DEFAULT NULL,
	`ROUND` INT(10) NULL DEFAULT NULL,
	`CRT_DT` DATETIME NULL DEFAULT current_timestamp() ON UPDATE current_timestamp(),
	`UPD_DT` DATETIME NULL DEFAULT current_timestamp() ON UPDATE current_timestamp(),
	PRIMARY KEY (`SEASON_ID`) USING BTREE
)
COLLATE='utf8mb4_general_ci'
ENGINE=InnoDB
AUTO_INCREMENT=596
;

CREATE TABLE `tb_statistic` (
	`STAT_ID` BIGINT(20) NOT NULL AUTO_INCREMENT,
	`RAPID_STAT_ID` VARCHAR(50) NULL DEFAULT NULL COLLATE 'utf8mb4_general_ci',
	`MATCH_ID` BIGINT(20) NULL DEFAULT NULL,
	`HOME_SERVE_ACE` SMALLINT(6) NULL DEFAULT NULL,
	`AWAY_SERVE_ACE` SMALLINT(6) NULL DEFAULT NULL,
	`HOME_DOUBLE_FAULTS` SMALLINT(6) NULL DEFAULT NULL,
	`AWAY_DOUBLE_FAULTS` SMALLINT(6) NULL DEFAULT NULL,
	`HOME_SERVICE_POINTS_WON` SMALLINT(6) NULL DEFAULT NULL,
	`AWAY_SERVICE_POINTS_WON` SMALLINT(6) NULL DEFAULT NULL,
	`HOME_WINNERS_FOREHAND` SMALLINT(6) NULL DEFAULT NULL,
	`AWAY_WINNERS_FOREHAND` SMALLINT(6) NULL DEFAULT NULL,
	`HOME_WINNERS_BACKHAND` SMALLINT(6) NULL DEFAULT NULL,
	`AWAY_WINNERS_BACKHAND` SMALLINT(6) NULL DEFAULT NULL,
	`HOME_WINNERS_RETURN` SMALLINT(6) NULL DEFAULT NULL,
	`AWAY_WINNERS_RETURN` SMALLINT(6) NULL DEFAULT NULL,
	`HOME_ERRORS_FOREHAND` SMALLINT(6) NULL DEFAULT NULL,
	`AWAY_ERRORS_FOREHAND` SMALLINT(6) NULL DEFAULT NULL,
	`HOME_ERRORS_BACKHAND` SMALLINT(6) NULL DEFAULT NULL,
	`AWAY_ERRORS_BACKHAND` SMALLINT(6) NULL DEFAULT NULL,
	`HOME_ERRORS_RETURN` SMALLINT(6) NULL DEFAULT NULL,
	`AWAY_ERRORS_RETURN` SMALLINT(6) NULL DEFAULT NULL,
	`HOME_UNFORCED_ERRORS_FOREHAND` SMALLINT(6) NULL DEFAULT NULL,
	`AWAY_UNFORCED_ERRORS_FOREHAND` SMALLINT(6) NULL DEFAULT NULL,
	`HOME_UNFORCED_ERRORS_BACKHAND` SMALLINT(6) NULL DEFAULT NULL,
	`AWAY_UNFORCED_ERRORS_BACKHAND` SMALLINT(6) NULL DEFAULT NULL,
	`HOME_UNFORCED_ERRORS_RETURN` SMALLINT(6) NULL DEFAULT NULL,
	`AWAY_UNFORCED_ERRORS_RETURN` SMALLINT(6) NULL DEFAULT NULL,
	`CRT_DT` DATETIME NULL DEFAULT current_timestamp(),
	`UPD_DT` DATETIME NULL DEFAULT current_timestamp(),
	PRIMARY KEY (`STAT_ID`) USING BTREE
)
COLLATE='utf8mb4_general_ci'
ENGINE=InnoDB
;

CREATE TABLE `tb_tournament` (
	`TOURNAMENT_ID` BIGINT(20) NOT NULL AUTO_INCREMENT,
	`CATEGORY_ID` BIGINT(20) NULL DEFAULT NULL COMMENT 'ATP',
	`RAPID_TOURNAMENT_ID` VARCHAR(50) NULL DEFAULT NULL COMMENT 'RapidID' COLLATE 'utf8mb4_general_ci',
	`MATCH_TYPE` VARCHAR(50) NULL DEFAULT NULL COMMENT 'matchType' COLLATE 'utf8mb4_general_ci',
	`TOURNAMENT_NAME` VARCHAR(200) NULL DEFAULT NULL COMMENT 'competitionName' COLLATE 'utf8mb4_general_ci',
	`CITY` VARCHAR(50) NULL DEFAULT NULL COMMENT 'cityName' COLLATE 'utf8mb4_general_ci',
	`GROUND_TYPE` VARCHAR(50) NULL DEFAULT NULL COMMENT 'surfaceType' COLLATE 'utf8mb4_general_ci',
	`LOGO` VARCHAR(50) NULL DEFAULT NULL COLLATE 'utf8mb4_general_ci',
	`MOST_TITLES` VARCHAR(50) NULL DEFAULT NULL COLLATE 'utf8mb4_general_ci',
	`MOST_TITLE_PLAYER_ID` BIGINT(20) NULL DEFAULT NULL,
	`TITLE_HOLDER_ID` BIGINT(20) NULL DEFAULT NULL,
	`POINTS` INT(11) NULL DEFAULT NULL,
	`CRT_DT` DATETIME NULL DEFAULT current_timestamp() ON UPDATE current_timestamp(),
	`UPD_DT` DATETIME NULL DEFAULT current_timestamp() ON UPDATE current_timestamp(),
	PRIMARY KEY (`TOURNAMENT_ID`) USING BTREE
)
COLLATE='utf8mb4_general_ci'
ENGINE=InnoDB
AUTO_INCREMENT=296
;
